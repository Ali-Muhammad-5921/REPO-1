<h3>Categories</h3>
<a href="/admin/category/create" class="btn btn-info">Create New Category</a>
<table class="table table-hover table-striped">
    <thead>
        <tr>
            <th>_id</th>
            <th>Category Name</th>
            
        </tr>
    </thead>
    <tbody>
        <% for(let i=0; i < category.length ; i++ ){%>
            <tr>
                <td><%= category[i]._id %></td>
                <td><%= category[i].categoryName %></td>

                <td>
                    <a href="/admin/category/edit/<%= category[i]._id %>" class="btn btn-warning">Edit</a>
                    <a href="/admin/category/delete/<%= category[i]._id %>" class="btn btn-danger">Del</a>
                </td>
            </tr>
        <% } %> 
    </tbody>
</table>